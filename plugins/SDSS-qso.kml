<?xml version="1.0" encoding="UTF-8"?>
<kml xmlns="http://www.opengis.net/kml/2.2" hint="target=sky">
<Folder>
	<name>SDSS Stellar Query Layers</name>
	<open>1</open>
	<Folder>
		<name>SDSS Stellar Spectroscopic Layer</name>
		<open>1</open>
		<description>Layer to query top 100 brightest objects from data release 7 (DR7)</description>
		<NetworkLink>
			<name>SDSS Objects</name>
			<description>The database is queried any time the viewport stops.</description>
			<Link>
				<href>http://astrowiki.uchicago.edu/~sleitner/GS/cgi-bin/query-star-spec.pl?</href>
				<viewRefreshMode>onStop</viewRefreshMode>
				<viewRefreshTime>.75</viewRefreshTime>
			</Link>
		</NetworkLink>
	</Folder>
	<Folder>
		<name>SDSS QSO Spectroscopic Layer</name>
		<description>Layer to query top 100 brightest spectroscopic objects from data release 7 (DR7)</description>
		<NetworkLink>
			<name>SDSS Objects</name>
			<open>1</open>
			<description>The database is queried any time the viewport stops.</description>
			<Link>
				<href>http://astrowiki.uchicago.edu/~sleitner/GS/cgi-bin/query-qso-spec.pl?</href>
				<viewRefreshMode>onStop</viewRefreshMode>
				<viewRefreshTime>1.5</viewRefreshTime>
			</Link>
		</NetworkLink>
	</Folder>
</Folder>
</kml>
